<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- mybatis mapper파일 -->
	<!-- namespace, id, resultType 3가지가 중요 -->

<mapper namespace="questionAttachMapper">
<!-- namespace는 매퍼를 식별하기 위한 고유한 값 -->
	<!-- 1대1문의 첨부파일 등록 -->
	<insert id="insertFiles" parameterType="java.util.List">
		insert into question_attachtb(notice_no, saved_name, original_name) values
		<foreach collection="list" item="attach" separator=",">
			(
			#{attach.noticeNo},
			#{attach.savedName},
			#{attach.originalName}
			)
		</foreach>
	</insert>
	
</mapper>